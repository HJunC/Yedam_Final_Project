<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="co.yd.deval.project.mapper.ProjectRequestMapper">

    <insert id="insertProjectRequest"></insert>
    <update id="updateProjectRequest"></update>
    <delete id="deleteProjectRequest"></delete>

    <select id="selectProjectRequestAll" resultType="co.yd.deval.project.service.ProjectRequestVO">
        SELECT *
        FROM PROJECT_REQUEST
    </select>
    <select id="selectProjectRequest" resultType="co.yd.deval.project.service.ProjectRequestVO">
        SELECT *
        FROM PROJECT_REQUEST
        WHERE PROJECT_NO = #{projectNo}
    </select>
</mapper>