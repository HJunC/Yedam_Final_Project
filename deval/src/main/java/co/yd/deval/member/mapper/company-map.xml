<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.yd.deval.member.mapper.CompanyMapper">
	<select id="companySelect" resultType="co.yd.deval.member.service.CompanyVO">
